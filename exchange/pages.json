		{
			"path": "pages/exchange/publishChoose/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		//发布
		{
			"path": "pages/exchange/publish/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		//订单列表
		{
			"path": "pages/exchange/orderList/index",
			"style": {
				"enablePullDownRefresh": true, //设置为true表示当前页面开启下拉刷新
