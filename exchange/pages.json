			"style": {
				"navigationStyle": "custom"
			}
		},
		// 设置
		{
			"path": "pages/mine/select/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 上传凭证
		{
			"path": "pages/exchange/upload/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		// 查看凭证
		{
			"path": "pages/exchange/see/index",
			"style": {
				"navigationStyle": "custom"
			}
		}
